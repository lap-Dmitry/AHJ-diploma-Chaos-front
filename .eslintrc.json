{
    "env": {
      "jest": true,
      "browser": true,
      "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "no-plusplus": "off",
      "consistent-return":"off",
      "import/no-named-as-default": 0,
      "no-param-reassign": 0,
      "class-methods-use-this": 0,
      "import/no-cycle": 0,
      "no-tabs": 0,
      "no-mixed-spaces-and-tabs": 0
    }
  }